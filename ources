[32m+[m[32m                    @Override[m
[32m+[m[32m                    public void onInitializationComplete(@NonNull InitializationStatus initializationStatus) {[m
[32m+[m[32m                        Map<String, AdapterStatus> statusMap = initializationStatus.getAdapterStatusMap();[m
[32m+[m[32m                        for (String adapterClass : statusMap.keySet()) {[m
[32m+[m[32m                            AdapterStatus status = statusMap.get(adapterClass);[m
[32m+[m[32m                            assert status != null;[m
[32m+[m[32m                            Log.d("MyApp", String.format([m
[32m+[m[32m                                    "Adapter name: %s, Description: %s, Latency: %d",[m
[32m+[m[32m                                    adapterClass, status.getDescription(), status.getLatency()));[m
[32m+[m[32m                        }[m
[32m+[m
[32m+[m[32m                        // Start loading ads here...[m
[32m+[m[32m                    }[m
[32m+[m[32m                });[m
[32m+[m[32m            }else {[m
[32m+[m[32m                MobileAds.initialize(activity, new OnInitializationCompleteListener() {[m
[32m+[m[32m                    @Override[m
[32m+[m[32m                    public void onInitializationComplete(@NonNull InitializationStatus initializationStatus) {[m
[32m+[m[32m                    }[m
[32m+[m[32m                });[m
[32m+[m[32m            }[m
[32m+[m[32m        }[m
[32m+[m[32m    }[m
