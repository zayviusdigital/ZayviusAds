[32m+[m[32mpackage com.zayvius.zs_ads.ads;[m
[32m+[m
[32m+[m[32mimport android.app.Activity;[m
[32m+[m
[32m+[m[32mimport com.applovin.sdk.AppLovinPrivacySettings;[m
[32m+[m[32mimport com.google.android.ump.ConsentForm;[m
[32m+[m[32mimport com.google.android.ump.ConsentInformation;[m
[32m+[m[32mimport com.google.android.ump.ConsentRequestParameters;[m
[32m+[m[32mimport com.google.android.ump.UserMessagingPlatform;[m
[32m+[m[32mimport com.unity3d.ads.metadata.MetaData;[m
[32m+[m
[32m+[m[32mpublic class ZayviusAdsGDPR {[m
[32m+[m[32m    public static ConsentInformation consentInformation;[m
[32m+[m[32m    public static ConsentForm consentForm;[m
[32m+[m
[32m+[m[32m    /*Admob*/[m
[32m+[m[32m    public static void Admob(Activity activity){[m
[32m+[m[32m        if (ZayviusAdsOnOff.ad_admob) {[m
[32m+[m[32m            ConsentRequestParameters params = new ConsentRequestParameters[m
[32m+[m[32m                    .Builder()[m
[32m+[m[32m                    .setTagForUnderAgeOfConsent(false)[m
[32m+[m[32m                    .build();[m
[32m+[m
[32m+[m[32m            consentInformation = UserMessagingPlatform.getConsentInformation(activity);[m
[32m+[m[32m            consentInformation.requestConsentInfoUpdate([m
[32m+[m[32m                    activity,[m
[32m+[m[32m                    params,[m
[32m+[m[32m                    () -> {[m
[32m+[m[32m                        // The consent information state was updated.[m
[32m+[m[32m                        // You are now ready to check if a form is available.[m
[32m+[m[32m                        if (consentInformation.isConsentFormAvailable()) {[m
[32m+[m[32m                            loadForm(activity);[m
[32m+[m[32m                        }[m
[32m+[m[32m                    },[m
[32m+[m[32m                    formError -> {[m
[32m+[m[32m                        // Handle the error.[m
[32m+[m[32m                    });[m
[32m+[m[32m        }[m
[32m+[m[32m    }[m
[32m+[m
[32m+[m[32m    public static void loadForm(Activity activity){[m
[32m+[m[32m        UserMessagingPlatform.loadConsentForm([m
[32m+[m[32m                activity,[m
[32m+[m[32m                consentForm -> {[m
[32m+[m[32m                    if(consentInformation.getConsentStatus() == ConsentInformation.ConsentStatus.REQUIRED) {[m
[32m+[m[32m                        consentForm.show([m
[32m+[m[32m                                activity,[m
[32m+[m[32m                                formError -> {[m
[32m+[m[32m                                    // Handle dismissal by reloading form.[m
[32m+[m[32m                                    loadForm(activity);[m
[32m+[m[32m                                });[m
[32m+[m
[32m+[m[32m                    }[m
[32m+[m
[32m+[m[32m                },[m
[32m+[m[32m                formError -> {[m
[32m+[m[32m                    /// Handle Error.[m
[32m+[m[32m                }[m
[32m+[m[32m        );[m
[32m+[m[32m    }[m
[32m+[m
[32m+[m[32m    /*ApplovinMax*/[m
[32m+[m[32m    public static void ApplovinMax(Activity activity, boolean children ){[m
[32m+[m[32m        if (ZayviusAdsOnOff.ad_applovinmax) {[m
[32m+[m[32m            AppLovinPrivacySettings.setHasUserConsent(true, activity);[m
[32m+[m[32m            AppLovinPrivacySettings.setIsAgeRestrictedUser(children, activity);[m
[32m+[m[32m            AppLovinPrivacySettings.setDoNotSell(false, activity);[m
[32m+[m[32m        }[m
[32m+[m[32m    }[m
[32m+[m
[32m+[m[32m    /*ApplovinZone*/[m
[32m+[m[32m    public static void ApplovinZone(Activity activity, boolean children ){[m
[32m+[m[32m        if (ZayviusAdsOnOff.ad_applovinzone) {[m
[32m+[m[32m            AppLovinPrivacySettings.setHasUserConsent(true, activity);[m
[32m+[m[32m            AppLovinPrivacySettings.setIsAgeRestrictedUser(children, activity);[m
[32m+[m[32m        }[m
[32m+[m[32m    }[m
[32m+[m
[32m+[m[32m    /*Unity*/[m
[32m+[m[32m    public static void Unity(Activity activity){[m
[32m+[m[32m        if (ZayviusAdsOnOff.ad_unity) {[m
[32m+[m[32m            MetaData gdprMetaData = new MetaData(activity);[m
[32m+[m[32m            gdprMetaData.set("gdpr.consent", true);[m
[32m+[m[32m            gdprMetaData.commit();[m
[32m+[m[32m        }[m
[32m+[m[32m    }[m
[32m+[m[32m}[m
