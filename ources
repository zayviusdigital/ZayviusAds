[32m+[m[32m    public static void AppInReview(Activity activity) {[m
[32m+[m[32m        ReviewManager reviewManager = ReviewManagerFactory.create(activity);[m
[32m+[m[32m        Task<ReviewInfo> request = reviewManager.requestReviewFlow();[m
[32m+[m[32m        request.addOnCompleteListener(task -> {[m
[32m+[m[32m            if (task.isSuccessful()) {[m
[32m+[m[32m                ReviewInfo reviewInfo = task.getResult();[m
[32m+[m
[32m+[m[32m                Task<Void> flow = reviewManager.launchReviewFlow(activity, reviewInfo);[m
[32m+[m[32m                flow.addOnCompleteListener(task1 -> {[m
[32m+[m[32m                    // The flow has finished. The API does not indicate whether the user[m
[32m+[m[32m                    // reviewed or not, or even whether the review dialog was shown.[m
[32m+[m[32m                });[m
[32m+[m[32m            }[m
[32m+[m[32m        });[m
[32m+[m[32m    }[m
