[32m+[m[32mimport com.google.android.gms.ads.LoadAdError;[m
[32m+[m[32mimport com.google.android.gms.ads.interstitial.InterstitialAd;[m
[32m+[m[32mimport com.google.android.gms.ads.interstitial.InterstitialAdLoadCallback;[m
[32m+[m[32mimport com.unity3d.ads.IUnityAdsLoadListener;[m
[32m+[m[32mimport com.unity3d.ads.IUnityAdsShowListener;[m
[32m+[m[32mimport com.unity3d.ads.UnityAds;[m
[32m+[m[32mimport com.unity3d.ads.UnityAdsShowOptions;[m
[32m+[m
[32m+[m[32mimport java.util.concurrent.TimeUnit;[m
[32m+[m
[32m+[m[32mpublic class ZayviusAdsInterstitial {[m
[32m+[m[32m    /*Admob*/[m
[32m+[m[32m    public static InterstitialAd mInterstitialAd;[m
[32m+[m[32m    public static boolean loadingIklan=true;[m
[32m+[m[32m    public static Integer hitung=0;[m
[32m+[m
[32m+[m[32m    /*ApplovinMax*/[m
[32m+[m[32m    public static MaxInterstitialAd interstitialAd;[m
[32m+[m[32m    public static int retryAttempt;[m
[32m+[m[32m    public static boolean loadingIklanapmax=true;[m
[32m+[m[32m    public static Integer hitungapmax=0;[m
[32m+[m
[32m+[m[32m    /*ApplovinZone*/[m
[32m+[m[32m    public static AppLovinAd currentAd;[m
[32m+[m[32m    public static AppLovinInterstitialAdDialog interstitialAdZone;[m
[32m+[m[32m    public static boolean loadingIklanapzone=true;[m
[32m+[m[32m    public static Integer hitungapzone=0;[m
[32m+[m
[32m+[m[32m    /*Unity*/[m
[32m+[m[32m    public static boolean loadingIklanunity=true;[m
[32m+[m[32m    public static Integer hitungunity=0;[m
[32m+[m
[32m+[m[32m    public static void admob(Activity activity){[m
[32m+[m[32m        if (ZayviusAdsOnOff.ad_admob) {[m
[32m+[m[32m            AdRequest adRequest = new AdRequest.Builder().build();[m
[32m+[m[32m            InterstitialAd.load(activity, ZayviusAdsIDAdmob.Interstitialx, adRequest, new InterstitialAdLoadCallback() {[m
[32m+[m[32m                @Override[m
[32m+[m[32m                public void onAdLoaded(@NonNull InterstitialAd interstitialAd) {[m
[32m+[m[32m                    mInterstitialAd = interstitialAd;[m
[32m+[m[32m                    interstitialAd.setFullScreenContentCallback([m
[32m+[m[32m                            new FullScreenContentCallback() {[m
[32m+[m[32m                                @Override[m
[32m+[m[32m                                public void onAdDismissedFullScreenContent() {[m
[32m+[m[32m                                    mInterstitialAd = null;[m
[32m+[m[32m                                    admob(activity);[m
[32m+[m[32m                                }[m
[32m+[m
[32m+[m
[32m+[m[32m                                @Override[m
[32m+[m[32m                                public void onAdShowedFullScreenContent() {[m
[32m+[m
[32m+[m[32m                                }[m
[32m+[m[32m                            });[m
[32m+[m
[32m+[m[32m                }[m
[32m+[m
[32m+[m[32m                @Override[m
[32m+[m[32m                public void onAdFailedToLoad(@NonNull LoadAdError loadAdError) {[m
[32m+[m[32m                    // Handle the error[m
[32m+[m[32m                    mInterstitialAd = null;[m
[32m+[m[32m                    admob(activity);[m
[32m+[m
[32m+[m[32m                }[m
[32m+[m[32m            });[m
[32m+[m[32m        }[m
[32m+[m[32m    }[m
[32m+[m
[32m+[m[32m    public static void AdmobloadInterstitial(Activity activity, int interval_inter) {[m
[32m+[m[32m        if (ZayviusAdsOnOff.ad_admob) {[m
[32m+[m[32m            hitung++;[m
[32m+[m[32m            if (loadingIklan) {[m
[32m+[m[32m                admob(activity);[m
[32m+[m[32m                switch (ZayviusAdsBackup.backup_ad) {[m
[32m+[m[32m                    case "applovinmax":[m
[32m+[m[32m                        applovinmax(activity);[m
[32m+[m[32m                        break;[m
[32m+[m[32m                    case "unity":[m
[32m+[m[32m                        unity();[m
[32m+[m[32m                        break;[m
[32m+[m[32m                    case "applovinzone":[m
[32m+[m[32m                        applovinzone(activity);[m
[32m+[m[32m                        break;[m
[32m+[m[32m                }[m
[32m+[m[32m                loadingIklan = false;[m
[32m+[m[32m            }[m
[32m+[m[32m            if (hitung % interval_inter == 0) {[m
[32m+[m[32m                if (mInterstitialAd != null) {[m
[32m+[m[32m                    mInterstitialAd.show(activity);[m
[32m+[m[32m                }else {[m
[32m+[m[32m                    switch (ZayviusAdsBackup.backup_ad) {[m
[32m+[m[32m                        case "applovinmax":[m
[32m+[m[32m                            show_applovinmax();[m
[32m+[m[32m                            break;[m
[32m+[m[32m                        case "unity":[m
[32m+[m[32m                            UnityAds.show(activity, ZayviusAdsIDUnity.Interstitialx, new UnityAdsShowOptions(), new IUnityAdsShowListener() {[m
[32m+[m[32m                                @Override[m
[32m+[m[32m                                public void onUnityAdsShowFailure(String s, UnityAds.UnityAdsShowError unityAdsShowError, String s1) {[m
[32m+[m
[32m+[m[32m                                }[m
[32m+[m
[32m+[m[32m                                @Override[m
[32m+[m[32m                                public void onUnityAdsShowStart(String s) {[m
[32m+[m
[32m+[m[32m                                }[m
[32m+[m
[32m+[m[32m                                @Override[m
[32m+[m[32m                                public void onUnityAdsShowClick(String s) {[m
[32m+[m
[32m+[m[32m                                }[m
[32m+[m
[32m+[m[32m                                @Override[m
[32m+[m[32m                                public void onUnityAdsShowComplete(String s, UnityAds.UnityAdsShowCompletionState unityAdsShowCompletionState) {[m
[32m+[m
[32m+[m[32m                                }[m
[32m+[m[32m                            });[m
[32m+[m[32m                            break;[m
[32m+[m[32m                        case "applovinzone":[m
[32m+[m[32m                            show_applovinzone();[m
[32m+[m[32m                            break;[m
[32m+[m[32m                    }[m
[32m+[m[32m                }[m
[32m+[m[32m                loadingIklan = true;[m
[32m+[m[32m            }[m
[32m+[m[32m        }[m
[32m+[m[32m    }[m
[32m+[m
[32m+[m[32m    public static void show_admob(Activity activity){[m
[32m+[m[32m        if (mInterstitialAd != null) {[m
[32m+[m[32m            mInterstitialAd.show(activity);[m
[32m+[m[32m        }[m
[32m+[m[32m    }[m
[32m+[m
[32m+[m[32m    /*ApplovinMax*/[m
[32m+[m[32m    public static void applovinmax(Activity activity){[m
[32m+[m[32m        if (ZayviusAdsOnOff.ad_applovinmax) {[m
[32m+[m[32m            interstitialAd = new MaxInterstitialAd(ZayviusAdsIDApplovinMax.Interstitialx, activity);[m
[32m+[m[32m            interstitialAd.setListener(new MaxAdViewAdListener() {[m
[32m+[m[32m                @Override[m
[32m+[m[32m                public void onAdExpanded(MaxAd ad) {[m
[32m+[m
[32m+[m[32m                }[m
[32m+[m
[32m+[m[32m                @Override[m
[32m+[m[32m                public void onAdCollapsed(MaxAd ad) {[m
[32m+[m
[32m+[m[32m                }[m
[32m+[m
[32m+[m[32m                @Override[m
[32m+[m[32m                public void onAdLoaded(MaxAd ad) {[m
[32m+[m[32m                    retryAttempt = 0;[m
[32m+[m[32m                }[m
[32m+[m
[32m+[m[32m                @Override[m
[32m+[m[32m                public void onAdDisplayed(MaxAd ad) {[m
[32m+[m
[32m+[m[32m                }[m
[32m+[m
[32m+[m[32m                @Override[m
[32m+[m[32m                public void onAdHidden(MaxAd ad) {[m
[32m+[m[32m                    interstitialAd.loadAd();[m
[32m+[m[32m                }[m
[32m+[m
[32m+[m[32m                @Override[m
[32m+[m[32m                public void onAdClicked(MaxAd ad) {[m
[32m+[m
[32m+[m[32m                }[m
[32m+[m
[32m+[m[32m                @Override[m
[32m+[m[32m                public void onAdLoadFailed(String adUnitId, MaxError error) {[m
[32m+[m[32m                    retryAttempt++;[m
[32m+[m[32m                    long delayMillis = TimeUnit.SECONDS.toMillis((long) Math.pow(2, Math.min(6, retryAttempt)));[m
[32m+[m
[32m+[m[32m                    new Handler().postDelayed(new Runnable() {[m
[32m+[m[32m                        @Override[m
[32m+[m[32m                        public void run() {[m
[32m+[m[32m                            interstitialAd.loadAd();[m
[32m+[m[32m                        }[m
[32m+[m[32m                    }, delayMillis);[m
[32m+[m[32m                }[m
[32m+[m
[32m+[m[32m                @Override[m
[32m+[m[32m                public void onAdDisplayFailed(MaxAd ad, MaxError error) {[m
[32m+[m[32m                    interstitialAd.loadAd();[m
[32m+[m[32m                }[m
[32m+[m[32m            });[m
[32m+[m
[32m+[m[32m            // Load the first ad[m
[32m+[m[32m            interstitialAd.loadAd();[m
[32m+[m
[32m+[m[32m        }[m
[32m+[m[32m    }[m
[32m+[m
[32m+[m[32m    public static void ApplovinMaxloadInterstitial(Activity activity , int interval_inter) {[m
[32m+[m[32m        if (ZayviusAdsOnOff.ad_applovinmax) {[m
[32m+[m[32m            hitungapmax++;[m
[32m+[m[32m            if (loadingIklanapmax) {[m
[32m+[m[32m                applovinmax(activity);[m
[32m+[m[32m                switch (ZayviusAdsBackup.backup_ad) {[m
[32m+[m[32m                    case "admob":[m
[32m+[m[32m                        admob(activity);[m
[32m+[m[32m                        break;[m
[32m+[m[32m                    case "unity":[m
[32m+[m[32m                        unity();[m
[32m+[m[32m                        break;[m
[32m+[m[32m                    case "applovinzone":[m
[32m+[m[32m                        applovinzone(activity);[m
[32m+[m[32m                        break;[m
[32m+[m[32m                }[m
[32m+[m[32m                loadingIklanapmax = false;[m
[32m+[m[32m            }[m
[32m+[m[32m            if (hitungapmax % interval_inter == 0) {[m
[32m+[m[32m                if (interstitialAd.isReady()) {[m
[32m+[m[32m                    interstitialAd.showAd();[m
[32m+[m[32m                }else {[m
[32m+[m[32m                    switch (ZayviusAdsBackup.backup_ad) {[m
[32m+[m[32m                        case "admob":[m
[32m+[m[32m                            if (mInterstitialAd != null) {[m
[32m+[m[32m                                mInterstitialAd.show(activity);[m
[32m+[m[32m                            }[m
[32m+[m[32m                            break;[m
[32m+[m[32m                        case "unity":[m
[32m+[m[32m                            UnityAds.show(activity, ZayviusAdsIDUnity.Interstitialx, new UnityAdsShowOptions(), new IUnityAdsShowListener() {[m
[32m+[m[32m                                @Override[m
[32m+[m[32m                                public void onUnityAdsShowFailure(String s, UnityAds.UnityAdsShowError unityAdsShowError, String s1) {[m
[32m+[m[32m                                }[m
[32m+[m
[32m+[m[32m                                @Override[m
[32m+[m[32m                                public void onUnityAdsShowStart(String s) {[m
[32m+[m
[32m+[m[32m                                }[m
[32m+[m
[32m+[m[32m                                @Override[m
[32m+[m[32m                                public void onUnityAdsShowClick(String s) {[m
[32m+[m
[32m+[m[32m                                }[m
[32m+[m
[32m+[m[32m                                @Override[m
[32m+[m[32m                                public void onUnityAdsShowComplete(String s, UnityAds.UnityAdsShowCompletionState unityAdsShowCompletionState) {[m
[32m+[m
[32m+[m[32m                                }[m
[32m+[m[32m                            });[m
[32m+[m[32m                            break;[m
[32m+[m
[32m+[m[32m                        case "applovinzone":[m
[32m+[m[32m                            show_applovinzone();[m
[32m+[m[32m                            break;[m
[32m+[m[32m                    }[m
[32m+[m[32m                }[m
[32m+[m[32m                loadingIklanapmax = true;[m
[32m+[m[32m            }[m
[32m+[m
[32m+[m[32m        }[m
[32m+[m[32m    }[m
[32m+[m
[32m+[m[32m    public static void show_applovinmax(){[m
[32m+[m[32m        if (interstitialAd.isReady()) {[m
[32m+[m[32m            interstitialAd.showAd();[m
[32m+[m[32m        }[m
[32m+[m[32m    }[m
[32m+[m
[32m+[m[32m    /*ApplovinZone*/[m
[32m+[m[32m    public static void applovinzone(Activity activity){[m
[32m+[m[32m        if (ZayviusAdsOnOff.ad_applovinzone) {[m
[32m+[m[32m            interstitialAdZone = AppLovinInterstitialAd.create(AppLovinSdk.getInstance(activity), activity);[m
[32m+[m[32m            AppLovinSdk.getInstance(activity).getAdService().loadNextAdForZoneId(ZayviusAdsIDApplovinZone.Zone_Interstitialx, new AppLovinAdLoadListener() {[m
[32m+[m[32m                @Override[m
[32m+[m[32m                public void adReceived(AppLovinAd ad) {[m
[32m+[m[32m                    currentAd = ad;[m
[32m+[m[32m                }[m
[32m+[m
[32m+[m[32m                @Override[m
[32m+[m[32m                public void failedToReceiveAd(int errorCode) {[m
[32m+[m
[32m+[m[32m                }[m
[32m+[m[32m            });[m
[32m+[m
[32m+[m[32m        }[m
[32m+[m[32m    }[m
[32m+[m
[32m+[m[32m    public static void ApplovinZoneloadInterstitial(Activity activity , int interval_inter) {[m
[32m+[m[32m        if (ZayviusAdsOnOff.ad_applovinzone) {[m
[32m+[m[32m            hitungapzone++;[m
[32m+[m[32m            if (loadingIklanapzone) {[m
[32m+[m[32m                applovinzone(activity);[m
[32m+[m[32m                switch (ZayviusAdsBackup.backup_ad) {[m
[32m+[m[32m                    case "admob":[m
[32m+[m[32m                        admob(activity);[m
[32m+[m[32m                        break;[m
[32m+[m[32m                    case "unity":[m
[32m+[m[32m                        unity();[m
[32m+[m[32m                        break;[m
[32m+[m[32m                    case "applovinmax":[m
[32m+[m[32m                        applovinmax(activity);[m
[32m+[m[32m                        break;[m
[32m+[m[32m                }[m
[32m+[m[32m                loadingIklanapzone = false;[m
[32m+[m[32m            }[m
[32m+[m[32m            if (hitungapzone % interval_inter == 0) {[m
[32m+[m[32m                if ( currentAd != null ) {[m
[32m+[m[32m                    interstitialAdZone.showAndRender( currentAd );[m
[32m+[m[32m                }else {[m
[32m+[m[32m                    switch (ZayviusAdsBackup.backup_ad) {[m
[32m+[m[32m                        case "admob":[m
[32m+[m[32m                            if (mInterstitialAd != null) {[m
[32m+[m[32m                                mInterstitialAd.show(activity);[m
[32m+[m[32m                            }[m
[32m+[m[32m                            break;[m
[32m+[m[32m                        case "unity":[m
[32m+[m[32m                            UnityAds.show(activity, ZayviusAdsIDUnity.Interstitialx, new UnityAdsShowOptions(), new IUnityAdsShowListener() {[m
[32m+[m[32m                                @Override[m
[32m+[m[32m                                public void onUnityAdsShowFailure(String s, UnityAds.UnityAdsShowError unityAdsShowError, String s1) {[m
[32m+[m[32m                                }[m
[32m+[m
[32m+[m[32m                                @Override[m
[32m+[m[32m                                public void onUnityAdsShowStart(String s) {[m
[32m+[m
[32m+[m[32m                                }[m
[32m+[m
[32m+[m[32m                                @Override[m
[32m+[m[32m                                public void onUnityAdsShowClick(String s) {[m
[32m+[m
[32m+[m[32m                                }[m
[32m+[m
[32m+[m[32m                                @Override[m
[32m+[m[32m                                public void onUnityAdsShowComplete(String s, UnityAds.UnityAdsShowCompletionState unityAdsShowCompletionState) {[m
[32m+[m
[32m+[m[32m                                }[m
[32m+[m[32m                            });[m
[32m+[m[32m                            break;[m
[32m+[m[32m                        case "applovinmax":[m
[32m+[m[32m                            show_applovinmax();[m
[32m+[m[32m                            break;[m
[32m+[m[32m                    }[m
[32m+[m[32m                }[m
[32m+[m[32m                loadingIklanapzone = true;[m
[32m+[m[32m            }[m
[32m+[m
[32m+[m[32m        }[m
[32m+[m[32m    }[m
[32m+[m
[32m+[m[32m    public static void show_applovinzone(){[m
[32m+[m[32m        if ( currentAd != null ) {[m
[32m+[m[32m            interstitialAdZone.showAndRender( currentAd );[m
[32m+[m[32m        }[m
[32m+[m[32m    }[m
