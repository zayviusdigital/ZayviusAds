[32m+[m[32m                                    public void onUserEarnedReward(@NonNull RewardItem rewardItem) {[m
[32m+[m[32m                                        // Handle the reward.[m
[32m+[m[32m                                        int rewardAmount = rewardItem.getAmount();[m
[32m+[m[32m                                        String rewardType = rewardItem.getType();[m
