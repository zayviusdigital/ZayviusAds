[32m+[m[32m                }[m
[32m+[m
[32m+[m[32m                @Override[m
[32m+[m[32m                public void onAdDisplayFailed(MaxAd ad, MaxError error) {[m
[32m+[m[32m                    rewardedAd.loadAd();[m
[32m+[m[32m                }[m
[32m+[m[32m            });[m
[32m+[m
[32m+[m[32m            rewardedAd.loadAd();[m
[32m+[m
[32m+[m[32m        }[m
[32m+[m[32m    }[m
