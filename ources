[32m+[m[32m                {[m
[32m+[m[32m                    // AppLovin SDK is initialized, start loading ads[m
[32m+[m[32m                }[m
[32m+[m[32m            } );[m
[32m+[m[32m            if (mediation){[m
[32m+[m[32m                AdSettings.setDataProcessingOptions( new String[] {} );[m
[32m+[m[32m            }[m
[32m+[m[32m            if (mediationdebugger){[m
[32m+[m[32m                AppLovinSdk.getInstance( activity ).showMediationDebugger();[m
