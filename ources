[32m+[m[32m    /*Unity*/[m
[32m+[m[32m    public static void unity(){[m
[32m+[m[32m        if (ZayviusAdsOnOff.ad_unity) {[m
[32m+[m[32m            UnityAds.load(ZayviusAdsIDUnity.Interstitialx, new IUnityAdsLoadListener() {[m
[32m+[m[32m                @Override[m
[32m+[m[32m                public void onUnityAdsAdLoaded(String s) {[m
[32m+[m
[32m+[m[32m                }[m
[32m+[m
[32m+[m[32m                @Override[m
[32m+[m[32m                public void onUnityAdsFailedToLoad(String s, UnityAds.UnityAdsLoadError unityAdsLoadError, String s1) {[m
[32m+[m
[32m+[m[32m                }[m
[32m+[m[32m            });[m
[32m+[m[32m        }[m
[32m+[m[32m    }[m
[32m+[m
[32m+[m[32m    public static void UnityloadInterstitial(Activity activity , int interval_inter) {[m
[32m+[m[32m        if (ZayviusAdsOnOff.ad_unity) {[m
[32m+[m[32m            hitungunity++;[m
[32m+[m[32m            if (loadingIklanunity) {[m
[32m+[m[32m                unity();[m
[32m+[m[32m                switch (ZayviusAdsBackup.backup_ad) {[m
[32m+[m[32m                    case "admob":[m
[32m+[m[32m                        admob(activity);[m
[32m+[m[32m                        break;[m
[32m+[m[32m                    case "applovinmax":[m
[32m+[m[32m                        applovinmax(activity);[m
[32m+[m[32m                        break;[m
[32m+[m[32m                    case "applovinzone":[m
[32m+[m[32m                        applovinzone(activity);[m
[32m+[m[32m                        break;[m
[32m+[m[32m                }[m
[32m+[m[32m                loadingIklanunity = false;[m
[32m+[m[32m            }[m
[32m+[m[32m            if (hitungunity % interval_inter == 0) {[m
[32m+[m[32m                UnityAds.show(activity, ZayviusAdsIDUnity.Interstitialx, new UnityAdsShowOptions(), new IUnityAdsShowListener() {[m
[32m+[m[32m                    @Override[m
[32m+[m[32m                    public void onUnityAdsShowFailure(String s, UnityAds.UnityAdsShowError unityAdsShowError, String s1) {[m
[32m+[m[32m                        switch (ZayviusAdsBackup.backup_ad) {[m
[32m+[m[32m                            case "admob":[m
[32m+[m[32m                                if (mInterstitialAd != null) {[m
[32m+[m[32m                                    mInterstitialAd.show(activity);[m
[32m+[m[32m                                }[m
[32m+[m[32m                                break;[m
[32m+[m[32m                            case "applovinmax":[m
[32m+[m[32m                                show_applovinmax();[m
[32m+[m[32m                                break;[m
[32m+[m[32m                            case "applovinzone":[m
[32m+[m[32m                               show_applovinzone();[m
[32m+[m[32m                                break;[m
[32m+[m[32m                        }[m
[32m+[m[32m                    }[m
[32m+[m
[32m+[m[32m                    @Override[m
[32m+[m[32m                    public void onUnityAdsShowStart(String s) {[m
[32m+[m
[32m+[m[32m                    }[m
[32m+[m
[32m+[m[32m                    @Override[m
[32m+[m[32m                    public void onUnityAdsShowClick(String s) {[m
[32m+[m
[32m+[m[32m                    }[m
[32m+[m
[32m+[m[32m                    @Override[m
[32m+[m[32m                    public void onUnityAdsShowComplete(String s, UnityAds.UnityAdsShowCompletionState unityAdsShowCompletionState) {[m
[32m+[m
[32m+[m[32m                    }[m
[32m+[m[32m                });[m
[32m+[m[32m                loadingIklanunity = true;[m
[32m+[m
[32m+[m[32m            }[m
